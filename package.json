{
  "name": "front-end-candidate-lightbox",
  "version": "0.0.0",
  "description": "",
  "homepage": "https://github.com/davezuko/front-end-candidate-lightbox",
  "author": "David Zukowski <zukoweb@gmail.com> (http://zuko.me)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davezuko/front-end-candidate-lightbox.git"
  },
  "license": "MIT",
  "main": "server/index.js",
  "scripts": {
    "start": "node bin/start",
    "build": "NODE_ENV=production webpack",
    "serve": "NODE_ENV=production npm start",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "prettier": "prettier \"{src,tests}/**/*.{js,ts,tsx}\"",
    "prettier:fix": "npm run prettier -- --write",
    "lint": "npm run prettier",
    "lint:fix": "npm run prettier:fix"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.(spec|test).(ts|tsx)"
    ],
    "setupFiles": [
      "<rootDir>/tests/specs/setup.ts"
    ],
    "globals": {
      "__DEV__": false
    },
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "\\.(sass|scss|css)$": "tests/mocks/style-mock",
      "~(.*)$": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "express": "^4.16.2",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.27",
    "@types/react-dom": "^16.0.3",
    "awesome-typescript-loader": "^3.4.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "node-sass": "^4.7.2",
    "prettier": "^1.9.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "ts-jest": "^21.2.4",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.21.0"
  }
}
